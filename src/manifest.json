{
  "name": "Calendar Plus",
  "short_name": "Calendar Plus",
  "version": "2.2.0",
  "description": "Google Calendar extension that provides insights into where your time goes. Includes power user tools, data export and customization",
  "author": "Max Patiiuk",
  "manifest_version": 3,
  "icons": {
    "16": "./public/images/extension_icon_16x16.png",
    "32": "./public/images/extension_icon_32x32.png",
    "48": "./public/images/extension_icon_48x48.png",
    "128": "./public/images/extension_icon_128x128.png"
  },
  "action": {
    "default_title": "See where your time went"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/public/images/web_dark_rd_SI@2x.webp",
        "/public/images/web_light_rd_SI@2x.webp",
        "/dist/main.bundle.js.map",
        "/dist/background.bundle.js.map"
      ],
      "matches": ["https://calendar.google.com/*"]
    }
  ],
  "key": "",
  "current_locale": "en",
  "permissions": ["identity", "storage"],
  "background": {
    "service_worker": "./dist/background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/calendar/u/0/r/*"],
      "js": ["./dist/main.bundle.js"]
    }
  ],
  "cross_origin_embedder_policy": {
    "value": "require-corp"
  },
  "cross_origin_opener_policy": {
    "value": "same-origin"
  }
}
